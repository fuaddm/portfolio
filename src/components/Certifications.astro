---
import SectionTitle from './SectionTitle.astro';

import { content } from '../static-content';
import GeneralCardText from './GeneralCardText.astro';
---

<div class="inside-container flex flex-col gap-6">
  <SectionTitle title={content.certificationsTitle} />
  {content.certifications.map((certification) => <GeneralCardText {...certification} />)}
</div>
